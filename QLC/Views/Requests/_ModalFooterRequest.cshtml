
@model QLC.Models.UserViewModel.ModalFooter
@using QLC.Models.UserViewModel

<div class="modal-footer">
    <button data-dismiss="modal" id="@Model.CancelButtonID" class="btn btn-default" type="button">@Model.CancelButtonText</button>
    @if (!Model.OnlyCancelButton)
    {
        <button class="btn btn-success" id="AddRequest" type="submit">@Model.SubmitButtonText</button>

    }
</div>
